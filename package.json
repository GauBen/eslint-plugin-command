{
  "name": "eslint-plugin-command",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.5",
  "description": "Anthony extended ESLint rules",
  "author": "Anthony Fu <anthonyfu117@hotmail.com>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/antfu",
  "homepage": "https://github.com/antfu/eslint-plugin-command#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/antfu/eslint-plugin-command.git"
  },
  "bugs": "https://github.com/antfu/eslint-plugin-command/issues",
  "keywords": [
    "eslint-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.mjs",
      "require": "./dist/config.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts",
        "./dist/*"
      ],
      "./config": [
        "./dist/config.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "pnpm run dev && eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.15.0",
    "@antfu/ni": "^0.21.12",
    "@antfu/utils": "^0.7.7",
    "@types/eslint": "^8.56.10",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.12.7",
    "@types/semver": "^7.5.8",
    "@typescript-eslint/rule-tester": "^7.7.1",
    "@typescript-eslint/typescript-estree": "^7.7.1",
    "@typescript-eslint/utils": "^7.7.1",
    "ajv": "^8.12.0",
    "bumpp": "^9.4.0",
    "eslint": "^9.1.1",
    "eslint-define-config": "^2.1.0",
    "esno": "^4.7.0",
    "lint-staged": "^15.2.2",
    "lodash.merge": "4.6.2",
    "pnpm": "^9.0.5",
    "rimraf": "^5.0.5",
    "semver": "^7.6.0",
    "simple-git-hooks": "^2.11.1",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "unbuild": "^2.0.0",
    "vite": "^5.2.10",
    "vitest": "^1.5.0"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
